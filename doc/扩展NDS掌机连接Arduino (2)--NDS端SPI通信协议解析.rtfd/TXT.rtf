// SPI interrupt routine\cf0 \
\cf10 ISR (SPI_STC_vect)\cf0 \
\cf10 \{\cf0 \
\cf10 \'a0 byte c = SPDR;\cf0 \
\cf10 \'a0\cf0 \
\cf10 \'a0 switch (command)\cf0 \
\cf10 \'a0 \{\cf0 \
\cf10 \'a0 // no command? then this is the command\cf0 \
\cf10 \'a0 case 0:\cf0 \
\cf10 \'a0 \'a0 command = c;\cf0 \
\cf10 \'a0 \'a0 SPDR = 0;\cf0 \
\cf10 \'a0 \'a0 break;\cf0 \
\cf10 \'a0 \'a0\'a0\cf0 \
\cf10 \'a0 // add to incoming byte, return result\cf0 \
\cf10 \'a0 case 'a':\cf0 \
\cf10 \'a0 \'a0 SPDR = c + 15; \'a0// add 15\cf0 \
\cf10 \'a0 \'a0 break;\cf0 \
\cf10 \'a0 \'a0\'a0\cf0 \
\cf10 \'a0 // subtract from incoming byte, return result\cf0 \
\cf10 \'a0 case 's':\cf0 \
\cf10 \'a0 \'a0 SPDR = c - 8; \'a0// subtract 8\cf0 \
\cf10 \'a0 \'a0 break;\cf0 \
\cf10 \
\'a0 \} // end of switch\cf0 \
\cf10 \
\} \'a0// end of interrupt service routine (ISR) SPI_STC_vect\cf0 \
